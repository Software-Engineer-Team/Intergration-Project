{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "@react-native-community"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ]
  }
}
